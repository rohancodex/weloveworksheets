<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data tracking</title>

    <link rel="stylesheet" href="css/weloveworksheets.css">
    <link rel="preconnect" href="https://fonts.gstatic.com">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
</head>

<body class="bg-light" style="background-image:url(/img/blobs.svg) ;background-repeat: no-repeat; background-size:cover;">

    <!-- nav bar -->
    <div class="jumbotron bg-white text-dark shadow-sm round">
        <div class="container-fluid">
            <div class="row justify-content-center">

                <div class="col-md-2 d-flex justify-content-center">
                    <!-- Hav to add logo and stuff -->
                    <img src="/img/logo.png" alt="" height="55px" width="55px" class="align-self-start mt-1 pl-5 ml-5">
                </div>
                <div class="col-md-7 d-flex justify-content-end">
                    <!-- navigation code from bootstrap lib -->
                    <nav class="navbar sticky-top navbar-expand-lg navbar-light">
                        <div class="container-fluid">

                            <div class="collapse navbar-collapse d-flex justify-content-center" id="navbarNav">
                                <ul class="navbar-nav">
                                    <li class="nav-item">
                                        <a class="nav-link " aria-current="page" href="/dashboard">Home</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="/about">About us</a>
                                    </li>

                                    <li class="nav-item">
                                        <a class="nav-link" href="/files">Downloadables</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="/iep">IEP Info</a>
                                    </li>
                                </ul>

                            </div>
                        </div>
                    </nav>
                </div>
                <div class="col-2 d-flex justify-content-center mt-2">
                    <ul class="navbar-nav">
                        <li class="nav-item ml-5">
                            <a href="/logout">
                                <button type="submit" class="btn text-pastel-purple px-4 rounded rounded-pill" style=" background-color: #51548C; color:white;">
                                    Log Out
                                </button>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- navbar ends here -->




    <div class="bigWrapper container-fluid">
        <div class="row">
            <!-- side nav bar starts here -->
            <div class="col-2 align-self-left bg-white shadow-sm mt-0 fullHeight py-3">
                <!-- <div class="d-flex justify-content-center"> <img src="/img/logo.png" alt="" height="75px" width="75px" class="align-self-center"></div> -->

                <h5 class=" text-pastel-purple pt-2 px-3">Data Tracking</h5>
                <ul class="nav flex-column pt-1 px-1">

                    <hr>
                    <li class="nav-item">
                        <a class="nav-link active text-dark" href="/student"><i class="bi bi-person-fill  text-secondary"></i>
                    Students</a>
                    </li>
                    <li class="nav-item ">
                        <a class="nav-link text-dark" href="/worksheet"><i
                        class="bi bi-file-earmark-word-fill text-secondary"></i> Worksheets</a>
                    </li>
                    <li class="nav-item ">
                        <a class="nav-link text-dark" href="/datatracking"><i class="bi bi-graph-up text-secondary"></i>
                    Analytics</a>
                    </li>

                </ul>
                <h5 class=" text-pastel-purple pt-4 px-3">Account</h5>
                <ul class="nav flex-column pt-1 px-1">
                    <hr>
                    <li class="nav-item">
                        <a class="nav-link active text-dark" href="/profile"><i class="bi bi-person-circle text-secondary"></i>
                    Profile</a>
                    </li>
                    <li class="nav-item ">
                        <a class="nav-link text-dark" href="/logout"><i class="bi bi-box-arrow-in-left text-secondary"></i>
                    Sign out</a>
                    </li>
                </ul>
            </div>
            <!-- side bar ends here -->
            <div class="col-10 my-2 mx-0 px-1">

                <div class="row bg-white rounded shadow-sm mx-3 d-flex justify-content-center">
                    <div class="jumbotron-fluid px-5 bg-dark text-white rounded d-flex justify-content-center align-items-center">
                        <p class="py-1 mt-1">Dashboard</p>
                    </div>
                    <div class="col-12 mx-5 py-3 mb-2">
                        <div class="container">
                            <div class="row">
                                <div class="col-12">
                                    <canvas id="myChart" height="200px" width="400px"></canvas>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="row  bg-white rounded shadow-sm m-3 d-flex justify-content-center">
                    <div class="jumbotron-fluid px-5 bg-dark text-white rounded d-flex justify-content-center align-items-center">
                        <p class="py-1 mt-1">Students</p>
                    </div>

                    <!-- students and progress -->
                    <div class="container mt-2">
                        <div class="row mx-4">
                            <div class="col-5">
                                Student
                            </div>
                            <div class="col-7">
                                Progress
                            </div>
                        </div>
                    </div>
                    <!-- progress bar codes -->

                    <% if(student.length > 0) { %>

                        <% student.forEach(student=>{ %>
                            <a href="/datatracking/<%=student.name%>" style="text-decoration: none;">
                                <div class="container ml-4 mb-1">
                                    <div class="row border border-light bg-white shadow-sm rounded m-1 mx-3 p-3 d-flex align-items-center">
                                        <div class="col-5 ml-2">
                                            <%= student.name%>
                                        </div>
                                        <div class="class col-5">
                                            <div class="progress">
                                                <div style="width: <%=student.progress%>%" class="progress-bar  bg-warning" role="progressbar" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                                                    <%= student.progress%>%
                                                </div>

                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </a>
                            <% }); %>

                                <% } %>
                                    <!-- progress bar ends here -->
                                    <!-- progress bar codes -->

                                    <!-- <div class="container ml-4 mb-1">
                        <div class="row border border-light bg-white shadow-sm rounded m-1 mx-3 p-3 d-flex align-items-center">
                            <div class="col-5 ml-2">
                               Ross Gellar 
                            </div>
                            <div class="class col-5">
                                <div class="progress">
                                    <div style="width: 100%" class="progress-bar  bg-warning" role="progressbar" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                  </div>
                            </div>
                
                        </div>
                    </div> -->
                                    <!-- progress bar ends here -->
                </div>

            </div>
            <!-- end of the big row    -->
        </div>
    </div>
    <footer class="pastel-purple">
        <div>
            <div class="container mt-5"> <br>
                <div class="row d-flex align-items-center justify-content-center">
                    <div class="col-6 text-white d-flex py-5" style="justify-content: space-between;">
                        <a href="/Terms and Conditions.pdf" class="text-white" style="text-decoration: none;" target="_blank">Terms and conditions</a>
                        <a href="/returnspolicy.pdf" class="text-white" style="text-decoration: none;" target="_blank">Returns Policy</a>
                        <a href="/Privacy Policy.pdf" class="text-white" style="text-decoration: none;" target="_blank">Privacy Policy</a>
                    </div>

                </div>
            </div>
        </div>


    </footer>
    <script>
        var test = '<%= JSON.stringify(result) %>';
        // //console.log(typeof(test[0]));
        // const temp = test.split(',');
        var temp = JSON.parse(test);
        console.log(typeof(temp));
        console.log(typeof(test));
        let mychart = document.getElementById('myChart').getContext('2d');
        let linechart = new Chart(mychart, {
            type: 'line',
            data: {
                labels: [
                    'Monday',
                    'Tuesday',
                    'Wednesday',
                    'Thursday',
                    'Friday',
                    'Saturday',
                    'Sunday'
                ],
                datasets: [{
                    label: 'total class average',
                    backgroundColor: 'rgb(62,149,205,0.2)',
                    borderColor: 'aqua',
                    data: temp,
                    fill: true,
                }]
            },
            options: {
                plugins: {
                    legend: {
                        display: false,
                    }
                },
                scales: {
                    y: {
                        suggestedMin: 80,
                        suggestedMax: 100
                    }
                }
            }

        });
    </script>


    <script src="https://kit.fontawesome.com/ffcb45aa63.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script>
</body>

</html>