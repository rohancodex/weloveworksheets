    <script>
        var element = document.getElementById('element-to-print');
        var opt = {
            margin: 1,
            pagebreak: {
                mode: 'css',
                avoid: ['.row', 'li']
            },
            filename: 'myfile.pdf',
            image: {
                type: 'jpeg',
                quality: 1
            },
            html2canvas: {
                scale: 1
            },
            jsPDF: {
                unit: 'in',
                format: 'tabloid',
                orientation: 'landscape'
            }

        };

        function pdf() {
            html2pdf().set(opt).from(element).save();
        }
    </script>


